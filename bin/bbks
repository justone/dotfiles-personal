#!/bin/sh

#_(
  "exec" "bb" "-I" "-O" "$0" "$@"
  )

(set! *data-readers* {:default tagged-literal})

(if (seq *command-line-args*)
  (binding [*print-meta* true]
    (let [ks (mapv read-string *command-line-args*)]
      (run! prn (map #(select-keys % ks) user/*input*))))
  (println "usage: bbks :key1 :key2"))
